@charset "UTF-8";

/* Google Translate 상단 프레임(배너) 숨기기 */
.goog-te-banner-frame.skiptranslate {
    display: none !important;
    visibility: hidden !important; /* 추가 */
    height: 0 !important;         /* 추가 */
    overflow: hidden !important;   /* 추가 */
}

/* body 태그에 Google Translate가 추가하는 top 마진 및 위치 조정 제거 */
body {
    top: 0px !important;
    position: static !important; /* 중요: relative 등으로 변경될 수 있는 position을 static으로 */
    margin-top: 0px !important;
}

/* HTML 태그에 Google Translate가 추가하는 스타일 조정 */
html.translated-ltr,
html.translated-rtl {
    margin-top: 0px !important;
    padding-top: 0px !important;
    /* Google이 html 태그에 추가하는 다른 레이아웃 변경 스타일 초기화 (필요시) */
    box-sizing: content-box !important; /* box-sizing 변경 시도 방지 */
}

/* Google 번역 위젯 자체를 숨기기 (div#google_translate_element) */
#google_translate_element, .skiptranslate {
    display: none !important;
    visibility: hidden !important;
    height: 0 !important;
    overflow: hidden !important;
    position: absolute !important; /* 화면에서 완전히 치워버리기 */
    top: -9999px !important;
    left: -9999px !important;
}


