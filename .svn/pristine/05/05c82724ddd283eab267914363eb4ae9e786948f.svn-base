<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="kr.or.ddit.ddtown.mapper.emp.artist.IArtistMapper">
 
 	<select id="getAllArtists" resultType="kr.or.ddit.vo.artist.ArtistVO">
        SELECT
            A.ART_NO,
            A.ART_GROUP_NO,
            A.MEM_USERNAME,
            A.ART_NM,
            A.ART_CONTENT,
            A.ART_REG_DATE,
            A.ART_MOD_DATE,
            A.ART_PROFILE_IMG,
            AG.ART_GROUP_DEBUTDATE AS artDebutDate 
        FROM
            ARTIST A
        LEFT OUTER JOIN
            ARTIST_GROUP AG ON A.ART_GROUP_NO = AG.ART_GROUP_NO
        ORDER BY
            A.ART_NM ASC
    </select>
 	
	<select id="getArtistList" resultType="kr.or.ddit.vo.artist.ArtistVO">
		SELECT
			ART_NO
			, ART_GROUP_NO
			, MEM_USERNAME
			, ART_NM
			, ART_CONTENT
			, ART_REG_DATE
			, ART_MOD_DATE
			, ART_PROFILE_IMG
		FROM
			ARTIST
	</select>
    
 </mapper>